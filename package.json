{
  "name": "trickshare",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run build",
    "deploy": "npx cdk deploy --require-approval never",
    "deploy-pipeline": "npx cdk -a 'node infrastructure/pipeline-app.js' deploy --require-approval never",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.0.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "aws-amplify": "^6.0.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "husky": "^8.0.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
