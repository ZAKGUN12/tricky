{
  "name": "trickshare",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run build",
    "deploy": "npx cdk deploy --require-approval never",
    "deploy-pipeline": "npx cdk -a 'node infrastructure/pipeline-app.js' deploy --require-approval never",
    "populate-db": "node scripts/populate-db.js",
    "add-more-tricks": "node scripts/add-more-tricks.js",
    "setup-kudos": "node scripts/setup-kudos-table.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.13.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.10",
    "aws-amplify": "^6.15.7",
    "bcryptjs": "^3.0.2",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "husky": "^8.0.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
